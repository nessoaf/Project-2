<h1 class="profile">Create your very own decks!</h1>
<p class="profile">Select the card you want to add to load and see which decks its allowed into!</p>
<div class="container">
    <% mtg.cards.forEach(card => { %>
    <% if (card.imageUrl) { %>
    <!-- thank you Pete  -->

    <div class="cardshow">
        <a href="/mtg/<%= card.id %>">
            <h3 class="cards"><%= card.name %></h3><img src="<%= card.imageUrl %>" alt="<%= card.name %>">
        </a>
    </div>
    <% }}) %>

</div>

<form action="post"></form>